<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_practical_test01_var01_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="ro.pub.cs.systems.eim.practicaltest01var01.practicaltest01var01.PracticalTest01Var01MainActivity">

    <Button
        android:id="@+id/sud"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sud"
        android:gravity="center"
        android:layout_below="@+id/vest"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="26dp" />

    <Button
        android:id="@+id/nord"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/nord"
        android:gravity="center"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/sud"
        android:layout_alignStart="@+id/sud" />

    <Button
        android:id="@+id/vest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/vest"
        android:gravity="center"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp"
        android:layout_below="@+id/nord"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/est"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/est"
        android:gravity="center"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:layout_below="@+id/nord"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sud"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="22dp"
        android:id="@+id/textView" />

    <Button
        android:id="@+id/second_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sec_activity"
        android:gravity="center"
        android:layout_marginTop="22dp"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
